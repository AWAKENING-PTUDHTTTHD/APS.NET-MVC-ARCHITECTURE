@using UserManagement.Models
@{
    ViewBag.Title = "Home";
    var user = @Session["user"] as User;
}

<div class="container" style="margin-top:70px;">
            <ul class="list-group">
                
                    <li class="list-group-item"><strong>Real Name: </strong>@user.name</li>
                    <li class="list-group-item"><strong>Register date: </strong>@user.created_at</li>
                    <li class="list-group-item"><strong>Spent Time Since Register: </strong>@ViewData["SpentTime"]</li>
                
            </ul>
        </div>
    </div>
</div>

